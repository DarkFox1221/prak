<Page x:Class="PRAKTIKA.AdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PRAKTIKA"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminPage">

    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Left" VerticalAlignment="Center">
            <Button x:Name="BTNStock" Content="Склад" Click="BTNStock_Click"/>
            <Button x:Name="BTNDriver" Content="Водитель" Click="BTNDriver_Click"/>
            <Button x:Name="BTNBuyer" Content="Покупатель" Click="BTNBuyer_Click"/>
            <Button x:Name="BTNSuctomer" Content="Поставщик" Click="BTNSuctomer_Click"/>
            <Button x:Name="BTNDriverCustomer" Content="Водитель-Поставщик" Click="BTNDriverCustomer_Click"/>
            <Button x:Name="BTNDriverBuyer" Content="Водитель-Покупатель" Click="BTNDriverBuyer_Click"/>
            <Button x:Name="BTNStockBuyer" Content="Склад-Покупатель" Click="BTNStockBuyer_Click"/>
        </StackPanel>
    </Grid>
</Page>
